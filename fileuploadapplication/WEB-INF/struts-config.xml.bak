<!DOCTYPE struts-config PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
        "http://struts.apache.org/dtds/struts-config_1_3.dtd">
<struts-config>
<form-beans>
<form-bean type="com.pitambar.view.FileUploadForm" name="fileUploadForm" />
</form-beans>

<action-mappings>
<action path="/fileUploadAction" type="com.pitambar.controler.FileUploadAction" name="fileUploadForm" >
<forward name="success" path="/success.jsp" />
</action>
</action-mappings>
</struts-config>